<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="listMembers.MemberListController">
    <children>
        <TableView fx:id="tableView" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columns>
                <TableColumn fx:id="codeCol" prefWidth="75.0" text="Member Code" />
                <TableColumn fx:id="nameCol" prefWidth="75.0" text="Member Name" />
                <TableColumn fx:id="mobileCol" prefWidth="75.0" text="Mobile" />
                <TableColumn fx:id="emailCol" prefWidth="75.0" text="Email" />
                <TableColumn fx:id="availCol" prefWidth="75.0" text="Availability" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
    </children>
</AnchorPane>
